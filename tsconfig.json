{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/api/*": ["./app/api/*"],
      "@/admin/*": ["./app/admin/*"],
      "@/components/*": ["./app/components/*"],
      "@/genres/*": ["./app/genres/*"],
      "@/galleries/*": ["./app/galleries/*"],
      "@/idols/*": ["./app/idols/*"],
      "@/lib/*": ["./app/lib/*"],
      "@/news/*": ["./app/news/*"],
      "@/photos/*": ["./app/photos/*"],
      "@/videos/*": ["./app/videos/*"],
      "@/watch/*": ["./app/watch/*"],
      "@/models/*": ["./models/*"],
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "app/admin/layout.tsx.backup"
, "scripts/generate-admin-hash.ts"  ],
  "exclude": ["node_modules"]
}
